{
  "name": "kaggle-cli",
  "version": "0.1.0",
  "description": "Kaggle CLI (Reserved)",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run build -- --watch",
    "prebuild": "rimraf lib",
    "build": "tsc --project tsconfig.build.json --rootDir src --outDir lib",
    "build-stdio": "tsc-stdio --stdio --project tsconfig.build.json --rootDir src --outDir lib",
    "test": "jest",
    "preversion": "npm test"
  },
  "keywords": [
    "kaggle",
    "cli"
  ],
  "author": "Cole Chamberlain",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/invariant": "2.2.28",
    "@types/jest": "19.2.4",
    "@types/node": "7.0.31",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.6",
    "tslib": "^1.7.1",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "glob": "^7.1.2",
    "graceful-fs": "^4.1.11",
    "invariant": "^2.2.2",
    "mkdirp": "^0.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
